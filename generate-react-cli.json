{
  "usesTypeScript": true,
  "usesStyledComponents": false,
  "usesCssModule": true,
  "cssPreprocessor": "css",
  "testLibrary": "Testing Library",
  "component": {
    "default": {
      "path": "src/components",
      "withStyle": true,
      "withTest": true,
      "withStory": true,
      "withLazy": true
    },
    "uicmp": {
      "path": "src/components/ui",
      "withStyle": true,
      "withTest": true,
      "withStory": true,
      "withLazy": true,
      "customTemplates":{
        "component":"grc_templates/uitemplate/TemplateName.js"
      }
    },
    "lycmp": {
      "path": "src/components/layout",
      "withStyle": true,
      "withTest": true,
      "withStory": true,
      "withLazy": true,
      "customTemplates":{
        "component":"grc_templates/layouttemplate/TemplateName.tsx",
        "test":"grc_templates/layouttemplate/TemplateName.test.tsx",
        "story":"grc_templates/layouttemplate/TemplateName.stories.tsx"
      }
    },
    "fccmp": {
      "path": "src/components/functional",
      "withStyle": true,
      "withTest": true,
      "withStory": true,
      "withLazy": true,
      "customTemplates":{
        "component":"grc_templates/fctemplate/TemplateName.tsx"
      }
    }
  }
}